{
  "extends": ["astro/tsconfigs/base", "../../tsconfig.base.json"],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "~": ["src/*"]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "solid-js",
    "plugins": [
      {
        "name": "@astrojs/ts-plugin"
      },
    ],
    "verbatimModuleSyntax": true
  }
}
